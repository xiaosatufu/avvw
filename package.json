{
  "name": "avvw",
  "version": "1.2.2",
  "description": "Apicloud + Vue2 + Vant（有赞前端）+ Webpack4打包，是一个采用Vue数据绑定特性和Apicloud手机操控能力相结合的APP开发框架",
  "main": "index.js",
  "scripts": {
    "wifi-start": "apicloud wifiStart --port 23456",
    "wifi-stop": "apicloud wifiStop --port 23456",
    "wifi-dev": "apicloud wifiSync --project ./ --updateAll false --port 23456",
    "wifi-build": "webpack --config webpack.prod.js && apicloud wifiSync --project ./ --updateAll false --port 23456",
    "wifi-log": "apicloud wifiLog --port 23456",
    "support": "browserslist",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "author": "Marlon <124711483@qq.com>",
  "license": "MIT",
  "keywords": [
    "avvw",
    "apicloud",
    "vue",
    "vue2",
    "vant",
    "webpack",
    "app",
    "hybird",
    "ios",
    "android",
    "javascript",
    "nodejs"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/grapewheel/avvw"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "apicloud-cli": "^0.2.0",
    "autoprefixer": "^9.6.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "browserslist": "^4.5.2",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jsonp": "^0.2.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue-loader": "^15.5.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "async-validator": "^2.0.1",
    "vant": "^2.0.5",
    "vue": "^2.6.10",
    "vuelidate": "^0.7.4"
  }
}
